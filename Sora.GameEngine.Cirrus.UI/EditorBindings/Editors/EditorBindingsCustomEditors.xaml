<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pe="clr-namespace:System.Activities.Presentation.PropertyEditing;assembly=System.Activities.Presentation">
    <DataTemplate x:Key="ProcessorInlineEditor">
        <ComboBox Text="{Binding Value.ProcessorValue, Mode=TwoWay}" ItemsSource="{Binding Value.AvailableProcessors}" IsEditable="True" />
    </DataTemplate>

    <DataTemplate x:Key="ImporterInlineEditor">
        <ComboBox Text="{Binding Value.ImporterValue, Mode=TwoWay}" ItemsSource="{Binding Value.AvailableImporters}" IsEditable="True" />
    </DataTemplate>
</ResourceDictionary>